
{
  "manifest_version": 3,
  "name": "ToViewLater",
  "version": "1.0",
  "description": "Save links to view later.",
  "permissions": [
    "storage", 
    "activeTab",
    "tabs",
    "notifications"
  ],
  "action": {
     "default_popup": "frontend/index.html", 
    "default_icon": "frontend/public/icons/logo.png"
  },
  "icons": {
    
    "128": "frontend/public/icons/logo.png"
  }
}
