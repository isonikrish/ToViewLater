
{
  "manifest_version": 3,
  "name": "ToViewLater",
  "version": "1.0",
  "description": "Save links to view later.",
  "permissions": [
    "storage", 
    "activeTab",
    "notifications",
    "sidePanel",
    "tabs"
  ],
  "icons": {
    "128": "./icons/logo.png"
  },
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "side_panel": {
    "default_path": "index.html"
  }
}
