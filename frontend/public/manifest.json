{
  "manifest_version": 3,
  "name": "ToViewLater",
  "version": "1.0",
  "description": "Save links to view later.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      
      "128": "icons/logo.png"
    }
  },
  "icons": {
    
    "128": "icons/logo.png"
  },
  "options_page": "options.html"
}
