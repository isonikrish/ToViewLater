
{
  "manifest_version": 3,
  "name": "ToViewLater",
  "version": "1.0",
  "description": "Save links to view later.",
  "permissions": [
    "storage", 
    "activeTab"
  ],
  "icons": {
    "128": "./icons/logo.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      
      "128": "icons/logo.png"
    }
  }
}